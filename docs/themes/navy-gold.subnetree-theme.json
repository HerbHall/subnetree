    "status": {
      "status-online": "#4ade80",
      "status-degraded": "#FFC300",
      "status-offline": "#ef4444",
      "status-unknown": "#4A6A8A"
    },
    "charts": {
      "chart-green": "#4ade80",
      "chart-amber": "#FFC300",
      "chart-sage": "#8BA4C4",
