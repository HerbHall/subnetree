{
  "subnetree_theme": true,
  "version": 1,
  "name": "Navy Gold",
  "description": "Deep navy with bright gold accents for high contrast monitoring.",
  "base_mode": "dark",
  "layers": ["colors", "effects"],
  "tokens": {
    "backgrounds": {
      "bg-root": "#000814",
      "bg-surface": "#001126",
      "bg-card": "#001D3D",
      "bg-elevated": "#002952",
      "bg-hover": "rgba(255, 195, 0, 0.06)",
      "bg-active": "rgba(255, 195, 0, 0.10)",
      "bg-selected": "rgba(255, 195, 0, 0.08)"
    },
    "text": {
      "text-primary": "#F0EDE5",
      "text-secondary": "#8BA4C4",
      "text-muted": "#4A6A8A",
      "text-accent": "#FFC300",
      "text-warm": "#FFD60A",
      "text-inverse": "#000814"
    },
    "borders": {
      "border-subtle": "rgba(255, 195, 0, 0.08)",
      "border-default": "rgba(255, 195, 0, 0.15)",
      "border-strong": "rgba(255, 195, 0, 0.25)",
      "border-focus": "#FFC300"
    },
    "buttons": {
      "btn-primary-bg": "#FFC300",
      "btn-primary-hover": "#FFD60A",
