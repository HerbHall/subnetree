{
  "subnetree_theme": true,
  "version": 1,
  "name": "Classic Dark",
  "description": "Neutral dark theme with slate grays and blue accents.",
  "base_mode": "dark",
  "tokens": {
    "backgrounds": {
      "bg-root": "#1a1b26",
      "bg-surface": "#1e1f2b",
      "bg-card": "#252736",
      "bg-elevated": "#2a2b3d",
      "bg-hover": "rgba(91, 156, 246, 0.06)",
      "bg-active": "rgba(91, 156, 246, 0.10)",
      "bg-selected": "rgba(91, 156, 246, 0.08)"
    },
    "text": {
      "text-primary": "#e1e2e7",
      "text-secondary": "#a0a4b8",
      "text-muted": "#6b6f85",
      "text-accent": "#5b9cf6",
      "text-warm": "#e6a855",
      "text-inverse": "#1a1b26"
    },
    "borders": {
      "border-subtle": "rgba(91, 156, 246, 0.08)",
